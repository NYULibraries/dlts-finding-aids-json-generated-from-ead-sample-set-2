        "scopecontent": [
            {
                "id": "aspace_5be955dfa9cd4e4ee9a8d01aae27d549",
                "head": {
                    "value": "Scope and Contents"
                },
                "p": [
                    {
                        "value": "This collection contains papers, play scripts, stage set designs, costume designs, scrapbooks and course notes by Mary Agnes Brady, a 1939 graduate of the School of Education.  Brady's class notes consist of the history, theory, and practice of theater.  The two scrapbooks in the collection contain images of costumes and set layouts designed by Brady as well as a script of Romeo and Juliet edited by her."
                    }
                ]
            }
        ],
