        "bioghist": [
            {
                "id": "aspace_2bb56294077e548b35a602840c974927",
                "head": [
                    {
                        "value": "Biographical Note"
                    }
                ],
                "p": [
                    {
                        "value": "George Schwartz received his Bachelor of Science with honors in biology from New York University (NYU) in 1925. He graduated from Bellevue Medical College in 1929.  While at NYU, he participated in activities such as the Politics Club, the Menorah Society, the Junior Show, the Paint and Powder Club, and the Stuyvesant Club."
                    }
                ]
            }
