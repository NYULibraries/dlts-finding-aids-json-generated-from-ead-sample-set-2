        "scopecontent": [
            {
                "id": "aspace_0e9d3175ee29431a0b544cd346fe1d38",
                "head": {
                    "value": "Scope and Contents"
                },
                "p": [
                    {
                        "value": "The Law Alumni of Color Association (LACA) Records contain files dating from 2001 to 2003 from the LACA Board President, Linda Gadsby, including correspondence, Board meeting agendas, LACA member directories, and notes. Planning files include material related to the LACA annual dinner, mentor program, and scholarships. The records also contain photographs from the 2002 BLAPA spring dinner."
                    }
                ]
            }
