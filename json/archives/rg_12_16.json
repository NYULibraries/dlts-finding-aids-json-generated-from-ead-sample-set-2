        "bioghist": [
            {
                "id": "aspace_d27d391fc3b6c47650d9c172968b218f",
                "head": {
                    "value": "Historical Note"
                },
                "p": [
                    {
                        "value": "The Center for Student Life engages New York University students during their transition into campus life. The Center serves as a central point of information and campus-wide events, and helps students navigate various aspects of the university, including clubs, fraternities, sororities, class activities boards, and community service opportunities."
                    }
                ]
            }
