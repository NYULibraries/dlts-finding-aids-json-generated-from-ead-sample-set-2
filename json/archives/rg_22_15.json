        "bioghist": [
            {
                "id": "aspace_ba0dd4547e172e993f49e3ab27bd1cfd",
                "head": [
                    {
                        "value": "Historical Note"
                    }
                ],
                "p": [
                    {
                        "value": "The Reiss Center on Law and Security is a non-partison multidisciplinary research institute at NYU School of Law. Founded in 2003 as the Center on Law and Security and renamed in 2018, the center facilitates dialogue and produces research on legal, policy, and strategic issues in the field of national security. The Reiss Center holds public events, conferences, and workshops; hosts policy groups; produces research and writing in both academic and popular media; sponsors fellowships from leading experts in the field; and offers academic programs for students at NYU School of Law."
                    }
                ]
            }
        ],
