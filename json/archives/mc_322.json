                        "phystech": [
                            {
                                "id": "aspace_143d9103050bd7a4270eb15e54fdd250",
                                "head": [
                                    {
                                        "value": "Physical Characteristics and Technical Requirements"
                                    }
                                ],
                                "p": [
                                    {
                                        "value": "Due to technical or privacy issues, archived websites may not be exact copies of  the original website at the time of the web crawl. Certain file types will not be captured dependent on how they are embedded in the site. Other parts of websites that the crawler has difficulty capturing includes Javascript, streaming content, database-driven content, and highly interactive content. Full-Text \nsearches of archived websites are available at https://archive-it.org/organizations/567."
                                    }
                                ]
                            }
                        ],
                        "scopecontent": [
                            {
                                "id": "aspace_6591c0899def78d5cd077a27d09b715a",
                                "head": [
                                    {
                                        "value": "Scope and Contents"
                                    }
                                ],
                                "p": [
                                    {
                                        "value": "These web sites document the University's participation in a city-wide celebration of the 50th anniversary of the Stonewall Riots. It contains a listing of events, the \"Stonewall at 50\" documentary, videos of past events, a timeline of NYU and the LGBTQ+ community, and information on arts exhibits, lectures, film series, operas, and events at NYU study abroad sites.  It also includes the Liberal Studies Program's website on its events series to commemorate the Stonewall Riots and a press release on the AIDS Memorial Quilt display in the Mamdouha Bobst Gallery."
                                    }
                                ]
                            }
                        ]
