        "bioghist": [
            {
                "id": "aspace_2b153940cffdbbffbb1a0b779913e34d",
                "head": [
                    {
                        "value": "Historical Note"
                    }
                ],
                "p": [
                    {
                        "value": "In 2019, numerous NYU programs, exhibits, and performances celebrated and commemorated the 1969 Stonewall Uprising anniversary in a campaign of events that the University called Stonewall@50. The events and exhibits focused on various aspects of LGBTQ+ history and activism."
                    }
                ]
            }
