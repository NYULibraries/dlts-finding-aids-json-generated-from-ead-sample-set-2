                "head": [
                    {
                        "value": "Processing Information"
                    }
                ],
                "p": [
                    {
                        "value": "John Zarrillo incorporated the publications Guess and Nite News into this finding aid in December 2019. All publications are part of a continuous run of the student newspaper for the Washington Square College Evening Division. A finding aid for the Nite News had previously been published with the call number RG 39.190."
                    }
                ]
            }
