        "scopecontent": [
            {
                "id": "aspace_e75ec16fda00ae29197eaaf6b0728362",
                "head": {
                    "value": "Scope and Contents"
                },
                "p": [
                    {
                        "value": "This collection includes a scrapbook that contains clippings from various sources including news organizations, newspapers, and pamphlets, from between 1927 and 1939 that mention the Institute of International Finance. Each item is labeled with its source and date. \"The Bulletins of NYU's Institute of Finance, 1926-1970: A Review Article,\" by Samuel Katz, reprinted from the Journal of Economic Literature, is in the same box as the scrapbook."
                    }
                ]
            }
