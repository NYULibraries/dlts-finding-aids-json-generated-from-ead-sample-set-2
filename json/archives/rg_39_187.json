        "scopecontent": [
            {
                "id": "aspace_552e9f4daa720f891db069470e4e680e",
                "head": [
                    {
                        "value": "Scope and Contents"
                    }
                ],
                "p": [
                    {
                        "value": "This collection contains issues of student newspapers the Square Journal and Washington Square Journal dating from 1955-1973. The newspaper was printed daily and documented student life, university news, and local or international news with relevancy to NYU."
                    }
                ]
            }
