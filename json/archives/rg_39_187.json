        "bioghist": [
            {
                "id": "aspace_42c64fcff3a3e981aaded596e6fd7221",
                "head": {
                    "value": "Biographical / Historical"
                },
                "p": [
                    {
                        "value": "New York University has had many student newspapers since its founding. The University Heights campus and the Washington Square campus had separate student newspapers until 1973 when the University Heights campus was closed. The Washington Square Journal was preceded by the Washington Square Bulletin and succeeded by the Washington Square Daily News."
                    }
                ]
            }
