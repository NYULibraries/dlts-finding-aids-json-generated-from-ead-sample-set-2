        "scopecontent": [
            {
                "id": "aspace_96c757823c75f19a3fc334d3d8cb54fe",
                "head": [
                    {
                        "value": "Scope and Contents"
                    }
                ],
                "p": [
                    {
                        "value": "This collection contains a selection of masters theses written by students from the Department of Economics between 1971 and 1979. A full list of thesis authors and titles may be found at the Archives. This collection does not contain all graduate theses written by students but is, rather, a selection that was collected by the Department during this time period."
                    }
                ]
            }
