        "custodhist": [
            {
                "id": "aspace_05a60424ceb28e8642b908e478dcc081",
                "head": {
                    "value": "Custodial History"
                },
                "p": [
                    {
                        "value": "Photographs taken by Arthur Smithline were donated to E. Jon Revere by Suzanne Smithline Manion, Smithline's daughter, and her husband Glenn Manion. Revere, a former conductor of the NYU Glee Club, later gave the photographs to Jeffrey Leeds to be added to his collection."
                    }
                ]
            }
        ],
