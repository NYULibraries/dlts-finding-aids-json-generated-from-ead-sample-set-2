        "scopecontent": [
            {
                "id": "aspace_744b187e15bce50112b924ab9a539a73",
                "head": {
                    "value": "Scope and Contents"
                },
                "p": [
                    {
                        "value": "This collection consists of materials created and collected by Richard Hull documenting his involvement in academic policy and planning with the New York University Faculty Council and Senate. Materials include correspondence, photographs, membership lists, meeting minutes, memoranda, pamphlets, magazines, and reports from meetings of the Faculty Council. Topics present include student life, academic curriculum programming, proposals for new faculty positions, and the affirmative action committee activities."
                    }
                ]
            }
        ],
