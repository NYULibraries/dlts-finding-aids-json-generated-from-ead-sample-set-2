                "head": [
                    {
                        "value": "Provenance"
                    }
                ],
                "p": [
                    {
                        "value": "A part of the collection, spanning 1949-1951, was acquired from Richard R. Neill in 2002. Files from 1952 were transferred in the same year from Princeton University's Seeley G. Mudd Manuscript Library."
                    }
                ]
            }
