        "scopecontent": [
            {
                "id": "aspace_50746cde87a8ce1bcfab7f9e06127a0c",
                "head": {
                    "value": "Scope and Content"
                },
                "p": [
                    {
                        "value": "The Albert W. Ferris Papers consist of notebooks related to NYU fraternities during the late nineteenth century.  The collection chronicles Ferris' time as a student at NYU, in particular his involvement in Phi Beta Kappa, Delta Upsilon, and Psi Upsilon. Notebooks list fraternity member honors, senior class presidents, and faculty reports."
                    }
                ]
            }
