        "accessrestrict": [
            {
                "id": "aspace_6809667d33489b17107bd7dda2b78761",
                "head": {
                    "value": "Conditions Governing Access"
                },
                "p": [
                    {
                        "value": "Administrative records and unpublished reports of New York University are closed for a period of 20 years from the date of their creation. Access to files spanning multiple years will be opened to researchers based on the date of the most recent materials. Board of Trustees records are closed for 35 years from the date of creation.  Materials related to personnel, grievances, job and fellowship searches and applications, and all files that fall under the federal Family Educational Rights and Privacy Act (FERPA) are permanently restricted.  Additional restrictions may apply to other materials in this collection.  For questions regarding specific restrictions, please contact the University Archives."
                    }
                ]
            }
        ],
        "acqinfo": [
            {
                "id": "aspace_9a663a8ed8e3098ff9e4ec934e4bb326",
                "head": {
                    "value": "Immediate Source of Acquisition"
                },
                "p": [
                    {
                        "value": "Transferred from the office of Barnett W. (Russ) Hamberger, 2017. The accession number associated with this collection is 2017.042."
                    }
                ]
            }
        ],
        "arrangement": [
            {
                "id": "aspace_d607e171c69d1b74ea016a28717e6984",
                "head": {
                    "value": "Arrangement"
                },
                "p": [
                    {
                        "value": "Files are arranged in chronological order, followed by t-shirts, hat, and a video cassette stored in the back of the record carton. Memorabilia was rehoused by preservation and four VHS tapes were housed in a shared box."
                    }
                ]
            }
        ],
        "bioghist": [
            {
                "id": "aspace_390bfb63297964193ad5ff5f02709a0b",
                "head": {
                    "value": "Biographical Note"
                },
                "p": [
                    {
                        "value": "Barnett W. (Russ) Hamberger (1940- ) held various positions at New York University since 1965, including Director of Branch Campus Affairs, Assistant to the Vice President for Academic Planning, and Assistant Vice Chancellor for Student Affairs. He received his bachelor's degree from Colgate University and M.A. from New York University. In the 1970s, after serving in various positions within the department of Academic Program Review, he became Associate Provost. While in this position, Hamberger reviewed new programs, served as liaison to the New York State Education Department on academic matters and to the University's regional accrediting agency. He also administered NYU's compliance with the Family Educational Rights and Privacy Act (FERPA) and managed staff who monitored academics and eligibility for NYU's athletics program. Hamberger also served as NYU's voting delegate to the University Athletic Association from its inception in 1985. He retired from his position in 2016."
                    }
                ]
            }
        ],
