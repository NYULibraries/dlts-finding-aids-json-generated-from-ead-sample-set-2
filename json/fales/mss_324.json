        "bioghist": [
            {
                "id": "aspace_423c039d140efe8ff32ea8550a783aa6",
                "head": [
                    {
                        "value": "Biographical Note"
                    }
                ],
                "p": [
                    {
                        "value": "Kezia Snyder is a New York City based natural foods chef and wellness consultant. Snyder has cooked healthy, organic, local, sustainable and macrobiotic food and taught the principles of a balanced, healthy, globally conscious lifestyle for over thirty years. She has been involved with the macrobiotic community in New York and the East Coast since she began reading books written by George Ohsawa, the founder of the macrobiotic diet, in 1969."
                    },
                    {
                        "value": "\nSources: "
                    },
                    {
                        "value": "https://www.facebook.com/KeziaSnyderHealthyLiving/"
                    },
                    {
                        "value": "http://keziasnyder.blogspot.com/"
                    }
                ]
            }
        ],
        "controlaccess": [
            {
                "subject": [
