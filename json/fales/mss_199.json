        "processinfo": [
            {
                "id": "aspace_0754e182858bf988d93571c9f8279599",
                "head": [
                    {
                        "value": "Processing Information"
                    }
                ],
                "p": [
                    {
                        "value": "Materials were placed in new acid-free folders and arranged chronologically. The 2009 accretion included one pamphlet that was added to Series I and two t-shirts and two VHS tapes that were added to Series II."
                    }
                ]
            }
        ],
        "scopecontent": [
            {
                "id": "aspace_14e35eb285dce8d8cdf582eb263ef19e",
                "head": [
                    {
                        "value": "Scope and Contents"
                    }
                ],
                "p": [
                    {
                        "value": "The collection is arranged in two series. The materials in Series I include brochures, pamphlets, postcards, and magazines from the 1980s, 1990s, and early 2000s. Series II contains condoms, safe sex kits, t-shirts, and two VHS tapes. The materials focus on HIV/AIDS prevention but also include subjects such as safe sex and STD prevention, pregnancy, abstinence, drug use, teenagers and sex, menopause, and birth control. These materials were in the main published and distributed by health clinics and organizations such as Gay Men's Health Crisis and Planned Parenthood. The collection also includes several international pamphlets from Europe and Latin America."
                    }
                ]
            }
        ],
        "separatedmaterial": [
            {
                "id": "aspace_cb69aa18cfd36ecb674c8b6ce376995a",
                "head": [
                    {
                        "value": "Separated Materials"
                    }
                ],
                "p": [
                    {
                        "value": "The 2009 accretion included books that were removed and added to the Fales Library collection at the time of donation."
                    }
                ]
            }
        ],
