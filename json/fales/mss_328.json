                    {
                        "value": "Scope and Content"
                    }
                ],
                "p": [
                    {
                        "value": "The Alice Himelstein AREA Collection contains material related to New York's AREA nightclub, extant from 1983 to 1987. Included in the collection are original invitations, press materials, flyers, and objects, mailed to and collected by Alice Himelstein."
                    }
                ]
            }
