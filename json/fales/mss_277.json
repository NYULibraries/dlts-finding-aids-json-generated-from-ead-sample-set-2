        "bioghist": [
            {
                "id": "aspace_1ff0678d94fd88ee12a4cfdf1f8cde36",
                "head": [
                    {
                        "value": "Biographical / Historical"
                    }
                ],
                "p": [
                    {
                        "value": "A's Events began in 1979 as an art show and performance occuring weekly on Wednesday nights at a loft space at 330 Broome Street in Manhattan. The events combined a 2D art show with performances, music, readings, and other forms of artistic production. Arleen Schloss, a pioneer in NYC performance art, curated the series."
                    }
                ]
            }
        ],
