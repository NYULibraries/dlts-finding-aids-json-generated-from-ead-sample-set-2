        "bioghist": [
            {
                "id": "aspace_e3a3d9b588a020b95ba0ae944aaec464",
                "head": [
                    {
                        "value": "Historical Note"
                    }
                ],
                "p": [
                    {
                        "value": "Fisher \u0026 Levy was a New York City catering business established by Chip Fisher and Doug Levy in 1983. Fisher \u0026 Levy offered daily corporate catering services as well as individual takeout items from their storefront location in Sutton Place. The catering company focused on providing high-end meals and food items tailored to appeal to a sophisticated, corporate clientele. Chip Fisher, a former IBM salesperson, handled the overall business, while Doug Levy, chef, was in charge of the cooking."
                    }
                ]
            }
        ],
