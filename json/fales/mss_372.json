        "scopecontent": [
            {
                "id": "aspace_a81baebc494159c713ee5b668a7d90ac",
                "head": [
                    {
                        "value": "Scope and Contents"
                    }
                ],
                "p": [
                    {
                        "value": "This collection contains materials created and collected by the Fisher \u0026 Levy catering company. A majority of the collection contains press clippings regarding the company, and the owners Chip Fisher and Doug Levy. A smaller portion of the collection contains correspondence from customers, certificates, and menus. Materials date from 1978 to 2006."
                    }
                ]
            }
        ],
