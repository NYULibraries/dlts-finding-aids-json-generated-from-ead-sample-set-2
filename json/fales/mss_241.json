        "custodhist": [
            {
                "id": "aspace_0f481ca47ec2806770b57bede1fe8134",
                "head": [
                    {
                        "value": "Custodial History"
                    }
                ],
                "p": [
                    {
                        "value": "James Pugliese collected this material sometime in the 1960s. He often purchased estates for their book holdings; this material probably came from one such estate. Materials were most likely donated by his daughter Jaimee Uhlenbrock in March 2006."
                    }
                ]
            }
        ],
