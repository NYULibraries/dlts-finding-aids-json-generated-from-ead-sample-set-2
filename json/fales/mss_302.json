        "scopecontent": [
            {
                "id": "aspace_2c6644bdab6320aa6a4fedf8d22ca828",
                "head": [
                    {
                        "value": "Scope and Contents"
                    }
                ],
                "p": [
                    {
                        "value": "This collection consists of two dresses, three \"performance slips,\" and one pair of shoes. Wolfe wore the slips during the 1990s while performing with the band, Bratmobile. The dresses and shoes are consistent with the style of clothing women wore in the 1990s and early 2000s, which is associated with the third wave feminist punk movement."
                    }
                ]
            }
