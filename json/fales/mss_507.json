        "bioghist": [
            {
                "id": "aspace_bfc1a279e1ca7f9ab417f0808d5ac2fa",
                "head": {
                    "value": "Historical Note"
                },
                "p": [
                    {
                        "value": "The New York Transfer Company was based in New York City and provided services for the transfer and transportation of baggage within New York. Services included pickup and delivery of customers' baggage to and from hotels, train stations, and ports."
                    }
                ]
            }
        ],
