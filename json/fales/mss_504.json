        "bioghist": [
            {
                "id": "aspace_2d561a267835ce9aac5a7358db4248a0",
                "head": {
                    "value": "Biographical Note"
                },
                "p": [
                    {
                        "value": "K. Marie Kim is a composer, musician, and audio engineer based in Brooklyn, NY. She holds a dual degree in Piano Performance and Music Production \u0026 Engineering from Berklee College of Music. \nShe has participated in both solo work and collaborations with other artists in her musical projects. Her music combines her classical instrumental training with electronic instruments and techniques."
                    }
                ]
