        "bioghist": [
            {
                "id": "aspace_0e24ec1cb909411125dc733829018983",
                "head": [
                    {
                        "value": "Historical/Biographical Note"
                    }
                ],
                "p": [
                    {
                        "value": "LGLN was founded in New York City in 1987 by Desma Holcomb, Peter Tenney, Vivienne Freund, Harry Franklin and Shelley Ettinger as well as other lesbian and gay trade union activists to promote lesbian and gay rights in unions and labor solidarity in the gay community. Activities include the Coors Boycott Coalition, publishing Pride at Work, organizing lesbian and gay committees in unions, and resource referral, particularly regarding AIDS in the workplace and domestic partnership benefits."
                    }
                ]
            }
        ],
