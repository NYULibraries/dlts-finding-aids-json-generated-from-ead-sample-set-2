        "custodhist": [
            {
                "id": "aspace_017cff46a86a86bffd6df65c4b042a7a",
                "head": {
                    "value": "Custodial History"
                },
                "p": [
                    {
                        "value": "The provenance of the materials is varied.  Items were obtained through purchases, donations, standing orders with publishers, arrangements with labor unions and other organizations, exchanges with other libraries, and through ongoing collecting by Tamiment staff."
                    }
                ]
            }
        ],
