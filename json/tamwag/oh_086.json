        "processinfo": [
            {
                "id": "aspace_fda11079deb37114c1565fd1b798cc92",
                "head": [
                    {
                        "value": "Processing Information"
                    }
                ],
                "p": [
                    {
                        "value": "Decisions regarding arrangement, description, and physical interventions for this collection prior to 2020 are unknown. In 2020, the finding aid was made compliant with DACS and local standard. Additional description regarding the archived websites and oral histories were added to the finding aid."
                    }
                ]
            }
        ],
        "scopecontent": [
            {
                "id": "aspace_f0359c976bb6d2f665ee63336c6b7a5f",
                "head": [
                    {
                        "value": "Scope and Contents"
                    }
                ],
                "p": [
                    {
                        "value": "This collection is made up of interviews conducted by students in Claudia Schachter's labor history class in 2010. Participants in the interviews include Irene Abraham, Donny Antinelli, Sarah Brandston, Camacho, Nina Coban, Chris Galford, Robert Howell, Mark Imperato, Iorio, Steve Moser, Cheryl Camella Sealey, and James K. Smith. The collection also includes the website for the union, which contains information on benefits, political action campaigns, news, and their newsletter, the Wire. The website has been captured from 2009 onwards."
                    }
                ]
            }
        ],
