                "head": {
                    "value": "Scope and Contents"
                },
                "p": [
                    {
                        "value": "The collection contains published and unpublished writings by Turner -- newspaper articles, pamphlets, statements, newsletters, and circular letters -- the latter two often relating to the many Trotskyist organizations in which he participated."
                    }
                ]
            }
