                "id": "aspace_f582a4aac1a5ea646b76cb47d9630e4a",
                "head": {
                    "value": "Processing Information"
                },
                "p": [
                    {
                        "value": "At the time of accessioning, the collection was rehoused in archival boxes and folders, transcribing original folder titles, and described on the collection-level with a folder list. No other arrangement, description, or physical interventions have taken place."
                    }
                ]
            }
