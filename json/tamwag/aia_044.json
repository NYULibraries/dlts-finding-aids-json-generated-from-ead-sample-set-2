        "scopecontent": [
            {
                "id": "aspace_374494b11161a978ddb55483022b0556",
                "head": [
                    {
                        "value": "Scope and Content Note"
                    }
                ],
                "p": [
                    {
                        "value": "The collection consists of correspondence, press releases, invitations and reports generated by the Consulate General of Ireland's New York office.  Of note are Reports of the Proceedings from the New Ireland Forum (1983-1984) and the Anglo-Irish Agreement (1985) between England and Northern Ireland.  Also of interest are invitations to various members of the New York Irish American community to events sponsored by the Consulate."
                    }
                ]
            }
        ],
        "separatedmaterial": [
            {
                "id": "aspace_d84a21af4ccee4549f549a7340ffd9d9",
                "head": [
                    {
                        "value": "Separated Materials"
                    }
                ],
                "p": [
                    {
                        "value": "Issues of the \u003cspan class=\"ead-emph ead-emph-italic\"\u003eIrish Voice\u003c/span\u003e and the \u003cspan class=\"ead-emph ead-emph-italic\"\u003eIrish People\u003c/span\u003e were removed to the appropriate collections."
                    }
                ]
            }
        ],
