                "head": {
                    "value": "Conditions Governing Use"
                },
                "p": [
                    {
                        "value": "Any rights (including copyright and related rights to publicity and privacy) held by Walter Rosenblum were transferred to New York University in 2007 by Lisa Rosenblum. Permission to publish or reproduce materials in this collection must be secured from Tamiment Library and Robert F. Wagner Archives. Please contact\ntamiment.wagner@nyu.edu, (212) 998-2630."
                    }
                ]
            }
        ]
