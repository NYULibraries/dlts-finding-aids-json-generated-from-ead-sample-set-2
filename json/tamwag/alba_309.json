        "bioghist": [
            {
                "id": "aspace_c9a0a0f416dd1755c176d74264a9fb93",
                "head": [
                    {
                        "value": "Biographical Note"
                    }
                ],
                "p": [
                    {
                        "value": "Ernest Amatniek was born in 1915 in Estonia and in 1929, emigrated to the United States. Amatniek attended James Monroe High School in the Bronx and the City College of New York where he studied electrical engineering. In February 1937, Amatniek joined and served with the XV International Brigade, Transmissiones squad fighting for the Spanish Republic in the Spanish Civil War. Amatniek passed away in April 2005 at the age of 90 in Bucks County, Pennsylvania."
                    }
                ]
            }
        ],
