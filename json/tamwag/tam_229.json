        "scopecontent": [
            {
                "id": "aspace_d1ee3a4203ee5a9db828edd35c64b9c4",
                "head": [
                    {
                        "value": "Scope and Contents"
                    }
                ],
                "p": [
                    {
                        "value": "The collection contains (non-official) student newsletters, principally Emanon, which provide a near-daily account of the events,  a detailed list of teach-in programs and other student-created documents, and research materials on the invasion of Cambodia and the Vietnam war, principally Agency French Press dispatches."
                    }
                ]
            }
