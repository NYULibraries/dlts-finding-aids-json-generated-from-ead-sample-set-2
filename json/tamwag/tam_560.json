        "scopecontent": [
            {
                "id": "aspace_43c8b2f179810512adbfdc04f4eed75d",
                "head": [
                    {
                        "value": "Scope and Content Note"
                    }
                ],
                "p": [
                    {
                        "value": "This small unprocessed collection contains correspondence and writings, internal documents and printed ephemera from progressive and radical political parties, spanning the years 1973-1991, including the Citizens Party, the Green Party, the National Unity Party, the Socialist Party, John Anderson's 1980 Presidential candidacy, and a file from the National Committee for Independent Political Action."
                    }
                ]
            }
        ],
