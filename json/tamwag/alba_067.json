        "custodhist": [
            {
                "id": "aspace_b2da14f5420e4dc24912c7722335d378",
                "head": [
                    {
                        "value": "Provenance"
                    }
                ],
                "p": [
                    {
                        "value": "The James Lardner papers were donated to the Abraham Lincoln Brigade Archives in 1980 by his brother Ring Lardner, Jr. This collection came to New York University in January 2001 as part of the original acquisition of the ALBA collections, formerly housed at Brandeis University."
                    }
                ]
