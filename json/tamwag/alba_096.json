                "head": [
                    {
                        "value": "Scope and Content Note"
                    }
                ],
                "p": [
                    {
                        "value": "This collection consists entirely of letters from Siegel while in Spain to his friend and former Harvard classmate, Leo Epstein. Siegel, who signs his letters \"Jorg,\" writes of daily life and personal circumstances, as well as detailed descriptions of battles."
                    }
                ]
            }
