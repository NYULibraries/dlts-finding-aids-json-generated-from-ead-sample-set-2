        "scopecontent": [
            {
                "id": "aspace_d7e098c861378dcae226b450b0deaa22",
                "head": [
                    {
                        "value": "Scope and Contents"
                    }
                ],
                "p": [
                    {
                        "value": "This collection consists of materials collected by Kathleen Mulvey documenting her family history. The collection includes family correspondence, panoramas of Irish American social organization functions, and a scrapbook from Elizabeth Mulvey's first trip to Europe in the late 1920s, which included visits to Ireland, Italy, France, and Switzerland.  The collection also contains a banner celebrating John F. Kennedy's visit to Ireland in 1963."
                    }
                ]
            }
        ],
        "userestrict": [
            {
                "id": "aspace_27dddbdd5464dbbd0d5251f04f4e4a16",
                "head": [
                    {
                        "value": "Use Restrictions"
                    }
                ],
                "p": [
                    {
                        "value": "Any rights (including copyright and related rights to publicity and privacy) held by Kathleen Mulvey, the sister-in-law of Elizabeth Mulvey, were transferred to New York University in 1999. Permission to publish or reproduce materials in this collection must be secured from the Tamiment Library."
                    },
                    {
                        "value": "Material in the 2018 accretion, which was created in 1963, is in the public domain. Permission to publish or reproduce is not required."
                    }
                ]
            }
        ]
