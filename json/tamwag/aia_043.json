        "bioghist": [
            {
                "id": "aspace_73a07fbb2b6c16a95aedd22d4328d2b4",
                "head": [
                    {
                        "value": "Biographical Note"
                    }
                ],
                "p": [
                    {
                        "value": "Elizabeth Mulvey was born in the Bronx to a family of Irish immigrants: her father was a carpenter born in the Bronx and her mother was born in Wicklow. Mulvey worked as a teacher and later the principal of a public school in the Bronx."
                    },
                    {
                        "value": "Kathleen Mulvey was the first woman president of the Irish Institute and an active member of the County Carlow Society."
                    }
                ]
            }
        ],
