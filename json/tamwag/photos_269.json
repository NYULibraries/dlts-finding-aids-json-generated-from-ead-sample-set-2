        "processinfo": [
            {
                "id": "aspace_7fa20d8de34c39fc78237d859ed655a5",
                "head": {
                    "value": "Processing Information"
                },
                "p": [
                    {
                        "value": "Photographs and negatives were placed into acid-free folders. Loose slides were placed into acid-free negative sleeves and grouped with other sets of slides that remained in their original boxes. All of the items are stored in acid-free boxes."
                    }
                ]
            }
        ],
        "relatedmaterial": [
            {
                "id": "aspace_ce67e6c39539dd67f883cbbb9229c7b4",
                "head": {
                    "value": "Related Archival Materials"
                },
                "p": [
                    {
                        "value": "Marion Nestle Higley Hill Camp Photographs (PHOTOS 239); Peter Hall Higley Hill Camp Photographs (PHOTOS 271); Higley Hill Camp Records (TAM 447); Grace and Max Granich Papers (TAM 255)."
                    }
                ]
            }
        ],
        "scopecontent": [
            {
                "id": "aspace_86bad8f46206df487e7a8320129b10d9",
                "head": {
                    "value": "Scope and Content Note"
                },
                "p": [
                    {
                        "value": "The collection contains black and white photographs and negatives, slides, brochures, and holiday greeting cards from the Higley Hill Camp, from the late 1940s and late 1960s, with the bulk of the materials from 1957-1966. These images show children and adults engaged in activities at the camp, as well as group shots and portraits of campers and staff; they also document a fire that took place at the camp in 1948."
                    }
                ]
            }
        ],
        "userestrict": [
