                "head": [
                    {
                        "value": "Historical/Biographical Note"
                    }
                ],
                "p": [
                    {
                        "value": "Joel Shufro was an instructor at the Center for Labor Studies (Labor College) for the Empire State College from 1972 through 1977. Shufro was active as a representative and as an officer in the Empire State College Chapter of the union, United University Professions, Inc. He also served on a subcommittee of the Academic Personnel Committee of the College."
                    }
                ]
            }
