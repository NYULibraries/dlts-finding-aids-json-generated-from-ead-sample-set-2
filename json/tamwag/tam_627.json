        "processinfo": [
            {
                "id": "aspace_66e6406b646f0c5fc45c34f02f0d1028",
                "head": {
                    "value": "Processing Information"
                },
                "p": [
                    {
                        "value": "The Mitchell Loeb Papers (TAM 627) was  previously named the Mitchell Loeb Drawings (TAM 065) and then the Mitchell Loeb Photographs (PHOTOS 169).  Materials were arranged and described by the archivist."
                    }
                ]
            }
        ],
        "relatedmaterial": [
            {
                "id": "aspace_3789ef10429996bf68376283d0ef8e06",
                "head": {
                    "value": "Related Archival Materials"
                },
                "p": [
                    {
                        "value": "The Tamiment/Wagner Poster Collection (GRAPHICS 002) includes several posters produced by Mitchell Loeb; works produced by  Loeb can be found in the  Eugene V. Debs Papers (TAM 020) as well.  Other items produced by Mitchell Loeb are also held by the Kheel Center for Labor-Management Documentation and Archives at Cornell University in its International Ladies Garment Workers (ILGWU) Paintings, Photographs, and Memorabilia collection."
                    }
                ]
            }
        ],
        "scopecontent": [
            {
                "id": "aspace_7b66dd4031631d03cf8d4de151716e87",
                "head": {
                    "value": "Scope and Contents"
                },
                "p": [
                    {
                        "value": "The Mitchell Loeb Papers consist of 223 items total, covering a number of different meda.  It includes 35 posters, 141 drawings, seven graphic items, nine pamphlets, 23 black and white photographs, three 8x10 negatives and five newspaper clippings.  Dated items range from the mid 1930s to the mid 1950s and consist mainly of work done by Loeb himself for the International Ladies Garment Workers  Union (ILGWU) and the \u003cspan class=\"ead-title\"\u003eJewish Daily Forward\u003c/span\u003e newspaper.",
                        "title": [
                            {
                                "value": "Jewish Daily Forward",
                                "render": "italic"
                            }
                        ]
                    }
                ]
            }
        ],
        "userestrict": [
            {
                "id": "aspace_3a919ad5cdfcd6ca0f53ad1d55c4f2cd",
                "head": {
                    "value": "Conditions Governing Use"
                },
                "p": [
                    {
                        "value": "Tamiment Library and Robert F. Wagner Labor Archive has no information about copyright ownership for this collection and is not authorized to grant permission to publish or reproduce materials from it. Materials in this collection, which were created in 1927-1954, are expected to enter the public domain in 2074."
                    }
                ]
            }
        ]
