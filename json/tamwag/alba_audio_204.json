        "scopecontent": [
            {
                "id": "aspace_0f157e1866c9fbc2da5004f3f1059cd3",
                "head": [
                    {
                        "value": "Scope and Contents"
                    }
                ],
                "p": [
                    {
                        "value": "Seven oral histories conducted by Joseph Friedman in the early 1990s with Spanish Civil War veterans and one individual who aided veterans crossing from Canada into the United States.  Interviewees include Mims Adler, John Simon, Paul Burns, Milt Felsen, Ken Graeber, Sam Gonshak, and Lem Harris."
                    }
                ]
            }
