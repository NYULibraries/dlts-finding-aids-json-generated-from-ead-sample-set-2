        "bioghist": [
            {
                "id": "aspace_e2c64892117b666a049d20b983de6246",
                "head": {
                    "value": "Historical/Biographical Note"
                },
                "p": [
                    {
                        "value": "The Tara Association was founded in Milltown, New Jersey 1971 by Pat Flynn, Maura Geraghty, Peggy Cuddy-Bojitas, and Phyllis Powell. The organization has grown from an original group of 25 to over 200 members. The Tara Association is dedicated to the preservation of Irish heritage, including the arts, poetry, literature, drama, folklore, and the Irish language. The name is based on the Hill of Tara, the ritual site of ancient high kings in Ireland. In June 2014, the Tara Association ceased activities."
                    }
                ]
            }
        ],
        "controlaccess": {
