        "processinfo": [
            {
                "id": "aspace_8a4dcc85f0853f6f4c1479df452caec5",
                "head": [
                    {
                        "value": "Processing Information"
                    }
                ],
                "p": [
                    {
                        "value": "At the time of accessioning, materials were moved into archival housing and a collection-level record was created to describe the materials. No arrangement or other physical interventions were performed at this time.\nIn 2018 an accretion of 20 posters was added to the collection and described in the file list as Year of the Rooster Parade Posters."
                    }
                ]
            }
        ],
        "scopecontent": [
            {
                "id": "aspace_06cb093a21f8d506f000145632baf134",
                "head": [
                    {
                        "value": "Scope and Contents"
                    }
                ],
                "p": [
                    {
                        "value": "This collection consists of materials created and used for the Q-Wave contingent of the 2014 and 2015 Lunar New Year for All parades, a collaborative effort between Q-Wave, Project Reach NYC, and Gay Asian \u0026 Pacific Islander Men of New York (GAPIMNY) that encourages participation in Lunar New Year parades in the greater New York City area. Materials include a rainbow sash, two sheep costumes to celebrate the Year of the Sheep, and 14 demonstration posters with messages in Chinese, English, and Japanese promoting inclusivity and challenging homophobia and discrimination."
                    },
                    {
                        "value": "20 oversize posters added to the collection in 2018 feature signs and artwork from the 2017 Lunar New Year for All parade. Posters contain writing in English and Chinese celebrating the year of the rooster, which feature hand drawn and painted artwork by parade participants."
                    }
                ]
            }
        ],
        "userestrict": [
            {
                "id": "aspace_60ac0b9c751593ddfe2327fea83a3b40",
                "head": [
                    {
                        "value": "Conditions Governing Use"
                    }
                ],
                "p": [
                    {
                        "value": "Copyright (or related rights to publicity and privacy) for materials in this collection, created by Q-Wave or Lunare New Year For All, was not transferred to New York University. Permission to use materials must be secured from the copyright holder."
                    }
                ]
            }
        ]
