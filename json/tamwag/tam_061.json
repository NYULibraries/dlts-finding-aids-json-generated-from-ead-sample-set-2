        "scopecontent": [
            {
                "id": "aspace_2d96080bc52cf613177f652ce6b6b4b0",
                "head": {
                    "value": "Scope and Content Note"
                },
                "p": [
                    {
                        "value": "This collection contains minutes of the Party's three conventions (1922-1923), Executive Committee minutes (1922-1924), and General Campaign Committee minutes (1922). Prominent persons active in the ALP include: Joseph D. Cannon, Edward F. Cassidy, Nathan Fine, Julius Gerber, Marie B. MacDonald, James Oneal, and A.I. Shiplacoff."
                    }
                ]
            }
        ],
