        "bioghist": [
            {
                "id": "aspace_e93f4ffe4eb2496638d2e2122cdadff9",
                "head": {
                    "value": "Historical/Biographical Note"
                },
                "p": [
                    {
                        "value": "Henry J. Meyer was the United States National War Labor Board (NWLB) liaison officer for independent unions. During World War II he was vice-chairman of the NWLB's National Telephone Panel and chair of the Wage Stabilization Board."
                    }
                ]
            }
        ],
