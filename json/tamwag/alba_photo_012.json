        "custodhist": [
            {
                "id": "aspace_969fdca1a58e89737035ef8f03ca17a5",
                "head": {
                    "value": "Provenance"
                },
                "p": [
                    {
                        "value": "The James Neugass Photograph Collection was donated to the Abraham Lincoln Brigade Archives by Ben Iceland in 1984. This collection came to New York University in January 2001 as part of the original acquisition of Abraham Lincoln Brigade Archives collections, formerly housed at Brandeis University."
                    }
                ]
