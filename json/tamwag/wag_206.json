                "head": {
                    "value": "Processing Information"
                },
                "p": [
                    {
                        "value": "Photographs were separated from this collection during initial processing and were established as a separate collection, the Ted Zittel Photographs (PHOTOS 96). In 2013, the photograph collection was reincorporated into the Theodore Zittel Scrapbook and Photographs."
                    }
                ]
            }
