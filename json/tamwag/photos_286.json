        "processinfo": [
            {
                "id": "aspace_8ef3f2e21cb753771dfd4c4d9c0716f4",
                "head": [
                    {
                        "value": "Processing Information"
                    }
                ],
                "p": [
                    {
                        "value": "In 2013, two boxes of photographs from the collection the National Council of American-Soviet Friendship Photographs (PHOTOS 054) were repatriated to the National Council of American-Soviet Friendship Records (TAM 134).  Due to the unknown provenance of the remainder of PHOTOS 054, the Collection of Soviet Union Photograph Albums and Photographs (PHOTOS 286) was established as a discrete collection."
                    }
                ]
            }
        ],
        "relatedmaterial": [
            {
                "id": "aspace_09a4e6cbc7b45849cba7c50e118bec2e",
                "head": [
                    {
                        "value": "Related Archival Materials"
                    }
                ],
                "p": [
                    {
                        "value": "Materials relating to the Soviet Union can also be found in the National Council of American Soviet Friendship Records (TAM 134)."
                    }
                ]
