        "bioghist": [
            {
                "id": "aspace_2bb504d342070fd2713e3d37130bac8b",
                "head": {
                    "value": "Historical/Biographical Note"
                },
                "p": [
                    {
                        "value": "Born in England, Mark Starr (1894-1985) served from 1935-1960 as educational director for the International Ladies Garment Workers Union, and had a lifelong interest in adult and workers education. A liberal activist, he was also associated with the American Labor Party, Americans for Democratic Action, the League for Industrial Democracy, and the Liberal Party."
                    }
                ]
            }
        ],
