                    }
                ],
                "p": [
                    {
                        "value": "Tamiment Library has no information about copyright ownership for this collection and is not authorized to grant permission to publish or reproduce materials from it. Materials in this collection, which were created in 1961-1963, are expected to enter the public domain in 2084."
                    }
                ]
            }
