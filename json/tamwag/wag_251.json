                "head": {
                    "value": "Scope and Contents"
                },
                "p": [
                    {
                        "value": "IATSE, Local 52 (Motion Picture and Sound Mechanics) records include membership directory, Local 52 newsletters, 50th anniversary program, Executive Board minutes (1966-1990), budget meeting minutes, general membership meeting minutes, and by-laws."
                    }
                ]
