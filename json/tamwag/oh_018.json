        "bioghist": [
            {
                "id": "aspace_ab3b22300e17aad09c8c2beed9552459",
                "head": [
                    {
                        "value": "Historical/Biographical Note"
                    }
                ],
                "p": [
                    {
                        "value": "NYU undergraduate Bichiluyen Nguyen, herself a Vietnamese immigrant, conducted five of the interviews as part of an internship in the History Department in 1989. In 2003, five additional interviews regarding the labor movement in the Chinatown garment shops were taken by Winifred Chin, a visiting scholar at the Asian/Pacific/American Institute."
                    }
                ]
            }
