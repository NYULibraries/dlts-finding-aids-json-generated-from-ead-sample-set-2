        "scopecontent": [
            {
                "id": "aspace_46870cde696b0c7a94bbbbb74272e87d",
                "head": {
                    "value": "Scope and Contents"
                },
                "p": [
                    {
                        "value": "The collection consists of records of the Abraham Lincoln Brigade Archives (ALBA), an organization of supporters of the U.S. veterans of the Spanish Civil War, mostly dating from the 1980s and 1990s.  It includes administrative files, financial and fundraising records and correspondence originating in ALBA's New York and San Francisco offices and files relating to ALBA's African-American Veterans Project."
                    }
                ]
            }
        ],
        "userestrict": [
            {
                "id": "aspace_00d53db8dbdc0cb120ebb48143c29c53",
                "head": {
                    "value": "Conditions Governing Use"
                },
                "p": [
                    {
                        "value": "Any rights (including copyright and related rights to publicity and privacy) held by the Abraham Lincoln Brigade Archives (ALBA), were transferred to New York University in November 2000 by the ALBA Board of Governors. Permission to publish or reproduce materials in this collection must be secured from the Tamiment Library \u0026 Robert F. Wagner Labor Archives. For more information, contact tamiment.wagner@nyu.edu or 212-998-2630."
                    }
                ]
            }
        ]
    },
