        "bioghist": [
            {
                "id": "aspace_c529066dadba89e93374030b622ff0d6",
                "head": {
                    "value": "Biographical Note"
                },
                "p": [
                    {
                        "value": "Selma Ruth Chadwick (1904-1962) was a Polish American nurse. She completed her nursing training at Bellevue Hospital in 1928, served as a nurse in the Republican Medical Services during the Spanish Civil War, and later served as a nurse in the United States Army during World War II, eventually earning the rank of Captain."
                    }
                ]
            }
