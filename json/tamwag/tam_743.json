        "scopecontent": [
            {
                "id": "aspace_405e789a144e05d9c23c4ef66db5d4f8",
                "head": [
                    {
                        "value": "Content Description"
                    }
                ],
                "p": [
                    {
                        "value": "This collection consists of black and white photographic prints of various sizes, film negatives, and color slides all created by Anne Dockery, a photographer with Liberation News Service. Subjects include anti-Vietnam War marches, Gay Pride marches, Black Panthers demonstrations, the Republican National Convention, plus New York City, Northern Ireland, Hanoi, London, and Vermont."
                    }
                ]
            }
        ],
