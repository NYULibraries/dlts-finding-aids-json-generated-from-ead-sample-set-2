                ],
                "p": [
                    {
                        "value": "Any rights (including copyright and related rights to publicity and privacy) held by John Harte were transferred to New York University in 2003 by John Harte. Permission to publish or reproduce materials in this collection must be secured from Tamiment Library and Robert F. Wagner Archives. Please contact tamiment.wagner@nyu.edu, (212) 998-2630."
                    }
                ]
            }
        ]
