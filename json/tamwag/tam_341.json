        "scopecontent": [
            {
                "id": "aspace_750000e22b54d410d208eb5e08ffae38",
                "head": [
                    {
                        "value": "Scope and Content Note"
                    }
                ],
                "p": [
                    {
                        "value": "This typescript lists, by year, national, state, and local candidates for public office in partisan and non-partisan elections who publicly represented themselves as communists, whether or not they were so identified on the election ballot. The listings including the following information: office sought, candidate's name, votes received, percentage of votes received."
                    }
                ]
            }
        ],
