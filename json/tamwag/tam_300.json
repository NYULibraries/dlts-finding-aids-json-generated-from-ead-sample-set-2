                "head": {
                    "value": "Conditions Governing Use"
                },
                "p": [
                    {
                        "value": "Copyright (and realted rights to publicity and privacy) to materials in this collection created by Janet Pickard was not transferred to New York University. Permission to use materials must be secured from the copyright holder."
                    }
                ]
            }
        ]
