        "bioghist": [
            {
                "id": "aspace_38a9217bb9aa59b26ce99fa7d69e0a92",
                "head": {
                    "value": "Historical Note"
                },
                "p": [
                    {
                        "value": "New York Labor History Association (NYLHA) was founded in 1976 by trade unionists, academics, students, archivists, educators, labor editors, attorneys, and retirees, mostly from New York State. NYLHA encourages the study of workers and their organizations and serves as a bridge between past and present labor unionists and academics."
                    }
                ]
            }
