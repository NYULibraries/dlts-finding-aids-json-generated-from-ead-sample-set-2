        "bioghist": [
            {
                "id": "aspace_0adbcaf66fd8b73bea81c93e7c827af3",
                "head": {
                    "value": "Historical/Biographical Note"
                },
                "p": [
                    {
                        "value": "Communications Workers of America, Local 1103 received its charter on May 12, 1961 with 900 members. By the 1990s, the union had grown to representing over 2000 New York telephone workers in Westchester and Putnam counties. Daniel Keenan was president from 1964-1979 and Robert McCracken was president from 1989-2007. As of this writing (2014) Kevin Sheil is president of the union."
                    }
                ]
            }
