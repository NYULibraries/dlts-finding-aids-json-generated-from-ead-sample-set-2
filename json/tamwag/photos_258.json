        "scopecontent": [
            {
                "id": "aspace_aee732e6407bcc670ff7e22c4667462f",
                "head": [
                    {
                        "value": "Scope and Contents"
                    }
                ],
                "p": [
                    {
                        "value": "The collection consists of ten pages of laser photocopies of 13 black-and-white photographs dating from 1951 to 1963, with captions written by Pacuska. The images are mostly group shots of Pacuska and the wait staff at Camp Tamiment, though there are also images of Pacuska with other camp staff and with the guests he served. Pacuksa's captions provide biographical information on specific staff members."
                    }
                ]
            }
