                    "scopecontent": [
                        {
                            "id": "aspace_0eeecd2397af3b8a6b7e75fbd10ff41b",
                            "head": {
                                "value": "Scope and Content Note"
                            },
                            "p": [
                                {
                                    "value": "This series contains four items, including two black-and-white prints and two letters. Both photographs include Fagel Stern, dated 1924. Both letters are addressed to Phyllis Stern."
                                }
                            ]
                        }
                    ]
