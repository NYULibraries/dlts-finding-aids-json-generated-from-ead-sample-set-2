                        "scopecontent": [
                            {
                                "id": "aspace_1672c49eda88c5fc107059fa6e9ae367",
                                "head": [
                                    {
                                        "value": "Scope and Content Note"
                                    }
                                ],
                                "p": [
                                    {
                                        "value": "This series contains Stern's diary, maintained during her fatal pregnancy, in which personal and political concerns are co-mingled. The series also contains her political notebooks; letters from her brother Abram Judah, also a communist who lived in Havana before returning to the USSR, where he died in 1933; letters from her brother Beryl, who came to the U.S. from Ostrolenka in 1935, and letters from her brother Chiel, who remained in Poland. The collection also contains biographical material on Stern including a reminiscence, sketch, and obituary."
                                    }
                                ]
                            }
                        ]
