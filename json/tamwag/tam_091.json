        "scopecontent": [
            {
                "id": "aspace_535faee3336c8de2a4d7aa355cce28b3",
                "head": {
                    "value": "Scope and Content Note"
                },
                "p": [
                    {
                        "value": "This collection contains Stern's diary; two notebooks with comments on Jews, Zionism, and world affairs; correspondence from family members, friends, and colleagues; and photographs. The collection also contains biographical material."
                    }
                ]
            }
