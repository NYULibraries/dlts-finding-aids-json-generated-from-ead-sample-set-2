        "scopecontent": [
            {
                "id": "aspace_3a907baf56b5c6441ec69cdfd08661b3",
                "head": {
                    "value": "Scope and Contents"
                },
                "p": [
                    {
                        "value": "This collection contains the manuscript of the unpublished anthology, \"Behind Bars: The Prison Experiences of War Resisters,\" edited by David F. Greenberg and Beverly D. Houghton. The anthology contains essays by resisters to the Vietnam War regarding their experiences in various prisons and jails. The collection also includes material from the New American Movement (NAM): newspapers, internal discussion bulletins, and others."
                    }
                ]
            }
        ],
