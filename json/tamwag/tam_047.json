                "head": {
                    "value": "Conditions Governing Use"
                },
                "p": [
                    {
                        "value": "Copyright (or related rights to publicity and privacy) for materials in this collection, created by Harry Laidler, was not transferred to New York University. Permission to use materials must be secured from the copyright holder."
                    }
                ]
            }
        ]
