                "id": "aspace_ce8492a61b627d7016ea9f0a783e0ff2",
                "head": {
                    "value": "Arrangement"
                },
                "p": [
                    {
                        "value": "This collection has not been arranged by an archivist. The materials are arranged in the order in which they were received from the donor. Letters were received in packets organized by location of POW camp."
                    }
                ]
            }
