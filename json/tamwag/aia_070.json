        "processinfo": [
            {
                "id": "aspace_e20415f152ab94e6c213debabe67d5fa",
                "head": [
                    {
                        "value": "Processing Information"
                    }
                ],
                "p": [
                    {
                        "value": "Access copies of the interviews in this collection were created by staff members at Ireland House prior to transfer to the archives. The digital files were transferred to the library's digtial repository, and the access copies and partial transcripts were rehoused in archival boxes and folders, then arranged alphabetically. The collection was described by an archivist."
                    }
                ]
            }
        ],
