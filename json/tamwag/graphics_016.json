        "bioghist": [
            {
                "id": "aspace_5d9c478b12d4452b274a9aaeb97eb886",
                "head": [
                    {
                        "value": "Historical/Biographical Note"
                    }
                ],
                "p": [
                    {
                        "value": "Lea Lerner was a music teacher at Camp Wo-Chi-Ca for several years after World War II. Camp Wo-Chi-Ca (which stood for Workers Children's Camp) was a children's summer camp in New Jersey. The camp, which was founded in 1934 and ran through the early 1950s, was mainly organized by the International Workers Order (I.W.O.) and was affiliated with the Communist Party, USA."
                    },
                    {
                        "value": "During the 1950s, Lerner and her family provided a safe house for underground activists Gil Green and Henry Winston."
                    }
                ]
            }
        ],
