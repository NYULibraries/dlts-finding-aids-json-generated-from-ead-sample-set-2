        "processinfo": [
            {
                "id": "aspace_bd61a0f40dde8bdd79006af757fe7fd6",
                "head": {
                    "value": "Processing Information"
                },
                "p": [
                    {
                        "value": "Photographs were separated from this collection during initial processing and were established as a separate collection, the American Committee for Protection of Foreign Born Photographs (PHOTOS 174). In 2013, the photograph collection was reincorporated into the American Committee for Protection of Foreign Born Records. In 2013 the collection was rehoused into one manuscript box."
                    }
                ]
            }
        ],
        "relatedmaterial": [
            {
                "id": "aspace_f411da88efb12d5ea2c973d2ab2323b7",
