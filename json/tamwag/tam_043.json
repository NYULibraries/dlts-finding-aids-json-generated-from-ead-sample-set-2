                "head": {
                    "value": "Scope and Content Note"
                },
                "p": [
                    {
                        "value": "The records contain correspondence, calls for a conference scheduled for December 1967, statements of purpose, minutes, lists of supporters and material from conferences and meetings, including a preparatory meeting at Columbia University's Mudd Hall, in March 1967."
                    }
                ]
            }
