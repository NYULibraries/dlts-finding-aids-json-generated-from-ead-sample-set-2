        "scopecontent": [
            {
                "id": "aspace_2cf4dd7c115ca6588dc1977dfc6f1afc",
                "head": {
                    "value": "Scope and Contents"
                },
                "p": [
                    {
                        "value": "This collection contains one letter from C.I. Taylor, an American volunteer who served in the Spanish Civil War. The letter describes Taylor's experience of being a machine gunner in the Tom Mooney Battalion, a name the machine gun company used for what is also known as the Lincoln Battalion. Also included are two typed transcripts of the letter and one illustration dated from 2016."
                    }
                ]
            }
